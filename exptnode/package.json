    "migrate:down": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert",
    "seed:config": "ts-node -r dotenv/config ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r dotenv/config ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js schema:sync"
